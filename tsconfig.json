{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "rootDir": ".",
    "outDir": "./starter/dist",
    "strict": true,
    "esModuleInterop": true,
    "types": ["node", "jasmine"],
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["starter/**/*.ts", "./spec/**/*.ts"],
  "exclude": ["node_modules", "starter/dist"]
}